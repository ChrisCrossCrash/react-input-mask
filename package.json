{
  "name": "react-input-mask",
  "description": "Masked input component for React",
  "version": "4.0.0",
  "homepage": "https://github.com/ChrisCrossCrash/react-input-mask",
  "license": "MIT",
  "author": "Christopher Kumm <chrislkumm@protonmail.com>  - forked from https://github.com/sanniassin/react-input-mask (thanks to Nikita Lobachev - @sanniassin)",
  "keywords": [
    "react",
    "input",
    "mask",
    "masked",
    "react-component"
  ],
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/plugin-transform-proto-to-assign": "^7.18.9",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.2.0",
    "console-polyfill": "^0.3.0",
    "cross-env": "^5.2.1",
    "eslint": "^8.27.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^4.0.2",
    "mocha": "^6.2.2",
    "prettier": "^2.7.1",
    "puppeteer": "^1.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-terser": "^5.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "main": "index.js",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "cross-env BABEL_ENV=rollup rollup -c",
    "start": "cross-env BABEL_ENV=test NODE_ENV=development webpack-dev-server",
    "prepare": "npm run lint && npm test && npm run clean && npm run build",
    "lint": "eslint --cache ./src ./tests ./*.js",
    "lint-fix": "eslint --cache --fix ./src ./tests ./*.js",
    "format": "prettier --write .",
    "test": "npm run build && npm run test:input && npm run test:server-render && npm run test:build",
    "test:input": "cross-env CHROME_BIN=/usr/bin/brave-browser NODE_ENV=test BABEL_ENV=test karma start",
    "test:server-render": "cross-env BABEL_ENV=test mocha --require @babel/register ./tests/server-render",
    "test:build": "cross-env BABEL_ENV=test mocha --require @babel/register ./tests/build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisCrossCrash/react-input-mask.git"
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "prop-types": "^15.7.2",
    "warning": "^4.0.3"
  }
}
